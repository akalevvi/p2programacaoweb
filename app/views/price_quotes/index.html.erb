<p id="notice"><%= notice %></p>

<h1>Price Quotes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @price_quotes.each do |price_quote| %>
      <tr>
        <td><%= link_to 'Show', price_quote %></td>
        <td><%= link_to 'Edit', edit_price_quote_path(price_quote) %></td>
        <td><%= link_to 'Destroy', price_quote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Price Quote', new_price_quote_path %>
